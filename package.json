{
  "name": "ataraxia-next",
  "version": "1.0.0",
  "description": "Ataraxia-Next: Modern serverless backend with proven business logic",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm run copy-files && npm run copy-prisma",
    "build:full": "tsc && npm run copy-files && npm run copy-prisma",
    "copy-files": "cp -r src/lib dist/ && cp -r src/shared dist/ && cp -r src/lambdas/auth/utils dist/lambdas/auth/ 2>/dev/null || true",
    "copy-prisma": "./scripts/copy-lambda-deps.sh",
    "dev:start": "./scripts/start-local.sh",
    "dev:stop": "./scripts/stop-local.sh",
    "dev:logs": "sam logs --stack-name ataraxia-next-local --tail",
    "local:start": "./start-local-api.sh",
    "local:stop": "./start-local-api.sh stop",
    "local:restart": "./start-local-api.sh restart",
    "local:status": "./start-local-api.sh status",
    "local:logs": "./start-local-api.sh logs",
    "local:test": "./start-local-api.sh test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:auth": "node scripts/test-auth-local.js",
    "validate:config": "node scripts/validate-deployment-config.js",
    "validate:pre-deploy": "./scripts/pre-deploy-check.sh",
    "generate:config-templates": "node scripts/config-template-generator.js",
    "cleanup:env-files": "./scripts/cleanup-env-files.sh",
    "consolidate:env-config": "node scripts/consolidate-env-config.js",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio",
    "migrate:universal-auth": "ts-node scripts/migrate-to-universal-auth.ts",
    "migrate:dry-run": "ts-node scripts/migrate-to-universal-auth.ts --dry-run",
    "deploy": "./scripts/deploy-with-validation.sh",
    "deploy:dev": "./scripts/deploy-with-validation.sh --environment dev",
    "deploy:staging": "./scripts/deploy-with-validation.sh --environment staging",
    "deploy:prod": "./scripts/deploy-with-validation.sh --environment prod",
    "deploy:dry-run": "./scripts/deploy-with-validation.sh --dry-run",
    "deploy:skip-validation": "./scripts/deploy-with-validation.sh --skip-validation",
    "deploy:cdk": "./scripts/deploy-with-cdk.sh",
    "deploy:cdk:dev": "./scripts/deploy-with-cdk.sh dev",
    "deploy:cdk:staging": "./scripts/deploy-with-cdk.sh staging",
    "deploy:cdk:prod": "./scripts/deploy-with-cdk.sh prod",
    "deploy:cognito": "./scripts/deploy-cognito.sh",
    "cdk:synth": "cd infrastructure && npx cdk synth",
    "cdk:diff": "cd infrastructure && npx cdk diff",
    "cdk:destroy": "cd infrastructure && npx cdk destroy"
  },
  "keywords": [
    "healthcare",
    "therapy",
    "serverless",
    "aws",
    "lambda"
  ],
  "author": "Ataraxia Team",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.975.0",
    "@aws-sdk/client-secrets-manager": "^3.975.0",
    "@aws-sdk/client-ssm": "^3.980.0",
    "@prisma/client": "^5.22.0",
    "aws-cdk": "^2.1103.0",
    "aws-cdk-lib": "^2.236.0",
    "aws-jwt-verify": "^5.1.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.13.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.2",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "tslib": "^2.8.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
